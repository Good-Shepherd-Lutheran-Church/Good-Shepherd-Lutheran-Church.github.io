{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "./programs.schema.json",
	"title": "School Programs",
	"description": "An array of objects that contain information about school programs at Good Shepherd, GB.",
	"type": "array",
	"items": {
		"type": "object",
		"properties": {
			"name": {
				"type": "string"
			},
			"description": {
				"type": "string"
			},
			"minAge": {
				"type": "number"
			},
			"maxAge": {
				"type": "number"
			},
			"hours": {
				"type": "string"
			},
			"days": {
				"type": "string"
			},
			"imageTerm": {
				"type": "string"
			}
		},
		"required": ["name", "description", "maxAge", "hours", "days"]
	}
}
